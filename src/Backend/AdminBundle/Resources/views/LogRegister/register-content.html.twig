<div class="container-fluid" style=" border-top: 2px solid #D5D5D5">
    <div class="container" style="margin-top: 40px;" >
        <div class="row">
            <div class="col-md-6 col-md-offset-3">
                <div class="col-md-8 col-md-offset-2">
                    <div class="btn-group btn-group-lg btn-group-justified">
                        <a href="javascript:void(0);" style="font-size: 15px; font-weight: bold;"
                           class="btn btn-default text-uppercase panel_left_different">
                            We are 100% free!<span style="color: #000000"> Curious why?</span>
                        </a>
                    </div>
                </div>
                <div class="col-md-12 no_padding">
                    <ul class="nav nav-pills">
                        <li class="active"><a aria-expanded="false" href="#dealer" data-toggle="tab">Register as a Dealerschip</a></li>
                        <li class=""><a aria-expanded="true" href="#privateseller" data-toggle="tab">Register as a Private Seller</a></li>
                    </ul>
                    <div id="myTabContent" class="tab-content" style="margin-top: 5px;">
                        <div class="tab-pane fade active in" id="dealer">
                            <fieldset>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group has-feedback">
                                            <input type="text" class="form-control" id="inputName"
                                                   placeholder="Fist name *" name="name" required>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-group has-feedback">
                                            <input type="text" class="form-control" id="inputLastName"
                                                   name="Lastname"
                                                   placeholder="Last Name *" required>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group has-feedback">
                                    <input type="email" class="form-control" id="inputEmail"
                                           name="Email"
                                           placeholder="Email *">
                                </div>
                                <div class="form-group has-feedback">
                                    <input type="password" name="pass" data-minlength="5"
                                           class="form-control"
                                           id="passwordInput1" placeholder="Password *"
                                           data-error="Must have more than 4 caracters" required>
                                    <div class="help-block with-errors"></div>
                                </div>
                                <div class="form-group has-feedback">
                                    <input type="password" name="pass" data-minlength="5"
                                           class="form-control"
                                           id="passwordInput2" placeholder="Confirm Password *"
                                           data-error="Must have more than 4 caracters"
                                           data-match="#passwordInput1"
                                           data-match-error="The passwords don't match " required>

                                    <div class="help-block with-errors"></div>
                                </div>
                                <div class="form-group has-feedback">
                                    <input type="text" class="form-control" id="inputphone" name="Phone"
                                           placeholder="Phone *" required="">
                                </div>
                                <div class="form-group">

                                </div>
                            </fieldset>
                        </div>
                        <div class="tab-pane fade" id="find_sol"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>