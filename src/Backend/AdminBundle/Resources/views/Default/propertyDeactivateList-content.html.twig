
<div id="container" class="container-fluid properties-container">
 {% if properties|length > 0 %}
 {% for property in properties %}
        
        
        <div class="list-row">
        
            <a id="{{property.serie}}" class="list-row-link-list" href="#" data-toggle="modal" data-target="#myModalDeactivation{{property.serie}}">
        <div  class="row" style="margin: 0 17px; padding: 15px 0; border-bottom: solid 2px #CFE1EF;">
            <div class="col-md-11 col-xs-12">{{property.serie}} - {{property.description.name}}   </div>
            <div class="col-md-1 col-xs-12"></div>
            
         </div>
             </a>
       
  
      </div> 
         {{ render(controller('AdminBundle:Default:deactivationForm',{'serie': property.serie })) }} 
    {% endfor %}
        <div class="row">
                <div class="col-md-12 col-xs-12">
                    {% set count = properties|length %}
                    <div class="text-right pull-right" style="margin: 10px 15px 10px 0px"><span style="padding: 7px;font-size: 14px;" class="label label-primary">{{ count }} 
                    {% transchoice  count %}
                    {0} There are no Properties|{1} Property Found|]1,Inf] Properties Found
                    {% endtranschoice %}
                    </span>
                    </div>
                </div>
            </div>
        
    {% else %} 

           
        <div class="row" style="margin: 20px 10px 0 ">
                    <div class="col-xs-12 col-md-12">
                       <div class="alert alert-info">You have no active listings</div>
                    </div>
                </div>
            
        
    {% endif %}
        
        
        
        
        
        
</div>

