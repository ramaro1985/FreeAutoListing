<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {% block metadescriptions %}{% endblock %}
    <title>{% block title %}My Dashboard{% endblock %}</title>

    {% block stylesheets %}
        <link href="{{ asset('bundles/common/css/bootstrap.css') }}" rel="stylesheet" />
        <link href="{{ asset('bundles/common/css/jquery-ui-1.10.4.custom.min.css') }}" rel="stylesheet" />

        <link href="{{ asset('bundles/common/css/dashboard_style.css') }}" rel="stylesheet" />
        <link href="{{ asset('bundles/common/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/common/css/jRating.jquery.min.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/common/css/fullcalendar.min.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/common/css/fullcalendar.print.css') }}" rel="stylesheet">
    {% endblock %}
    {% block javascripts %}
        <script  src="{{ asset('bundles/common/js/jquery-1.11.3.min.js') }}"></script>
        <script src="{{ asset('bundles/common/js/bootstrap.js') }}"></script>
        <script src="{{ asset('bundles/common/js/jquery-ui-1.10.4.min.js') }}"></script>
        <script src="{{ asset('bundles/common/js/transition.js') }}"></script>
        <script src="{{ asset('bundles/common/js/jRating.jquery.min.js') }}"></script>
        <script src="{{ asset('bundles/common/js/fullcalendar.js') }}"></script>
        <script src="{{ asset('bundles/common/js/tooltip.js') }}"></script>
        <script src="{{ asset('bundles/common/js/popover.js') }}"></script>


        <script src="{{ asset('bundles/common/js/jquery.bxslider.min.js') }}"></script>

        <!--<script src="https://maps.googleapis.com/maps/api/js?v=3.exp&signed_in=true&libraries=places&language=en"></script>-->
        <script>
            window.twttr = (function (d, s, id) {
                var js, fjs = d.getElementsByTagName(s)[0], t = window.twttr || {};
                if (d.getElementById(id))
                    return;
                js = d.createElement(s);
                js.id = id;
                js.src = "https://platform.twitter.com/widgets.js";
                fjs.parentNode.insertBefore(js, fjs);
                t._e = [];
                t.ready = function (f) {
                    t._e.push(f);
                };
                return t;
            }(document, "script", "twitter-wjs"));
        </script>

    {% endblock %}

    <link rel="icon" type="image/x-icon" href="{{ asset('bundles/common/images/free-auto.png') }}" />

</head>


<body>
<div id="fb-root"></div>
<script>
    if (window.location.hash == '#_=_') {
        window.location.hash = ''; // for older browsers, leaves a # behind
        history.pushState('', document.title, window.location.pathname); // nice and clean
        //e.preventDefault(); // no page reload
    }
</script>
<script>(function (d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id))
            return;
        js = d.createElement(s);
        js.id = id;
        js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.3";
        fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));</script>
{% block header %}{{ include('PrivateSellerBundle:Default:header.html.twig') }}{% endblock %}
<div class="container" style="margin-top: 20px;">
    <div class="row">

        <div class="col-md-2 col-xs-12 menu-owner no_padding">
            {% block menu %}{{ include('PrivateSellerBundle:Default:user-menu.html.twig') }}{% endblock %}
        </div>


        <div class="col-md-10 col-xs-12 content-owner" style="min-height: 700px">
            <div id="content-page">

                {% block body %}{% endblock %}
            </div>
        </div>

    </div>

</div>
{% block footer %}
    {{ include('CommonBundle:Default:footer.html.twig') }}
    {{ include('CommonBundle:Default:support_ticket.html.twig') }}
{% endblock %}





</body>
</html>
