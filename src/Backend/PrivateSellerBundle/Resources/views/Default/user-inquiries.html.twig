<div id="inquiries" class="container-fluid no_padding visible-md visible-lg hidden-xs hidden-sm">

    <div id="inquiries_carrusel" class="carousel slide" data-interval="false" data-ride="carousel">

        <!-- Bloque para las imÃ¡genes -->
        <div class="carousel-inner">
            {% set total_emails = emails|length %}
            {% set count = 0 %}
            {% set step_ = 0 %}
            {% if(total_emails==1) %}
                {% set step_ = 1 %}
                {% set jump = 1 %}
                {% set rest = 1 %}
            {% endif %}
            {% if(total_emails==2) %}
                {% set step_ = 2 %}
                {% set jump = 2 %}
                {% set rest = 0 %}
            {% endif %}
            {% if(total_emails >=3) %}
                {% set step_ = 3 %}
                {% set jump = 3 %}
                {% set rest = 0 %}
            {% endif %}

            {% if(step_ >0) %}
                {% for i in range(low=0, high=total_emails-rest, step=step_) %}
                    <div class="row_cars carrusel_tumb old item  {% if loop.first %} active {% endif %}">
                        {% for emails_n in emails|slice( i, jump ) %}

                            <div class="col-md-4 {% if loop.first %} first {% elseif loop.last %} last {% else %} middle {% endif %}"
                                 style="min-width: 317.766px ; min-height: 376px ">
                                <div class="col-md-12 previewbox borderBoxShadows"
                                     style="padding-bottom: 10px;padding-top: 10px;background-color: #ffffff">
                                    <div class="col-md-12 email_buttons hide">
                                        <a id="read{{ emails_n.id }}"
                                           image="<a class='thumbnail pull-left' style='margin-bottom: 0px'> {% if emails_n.vehicle.gallery is not null %} {% set pf = emails_n.vehicle.gallery.vehicleimages[0] %} <img src='{{ asset(pf.getwebpaththumbnailVehicle(emails_n.user.id,emails_n.vehicle.serie)) }}' class='img-responsive'/>{% else %} <img src='{{ asset('bundles/common/images/image-bg.jpg') }}' class='img-responsive' style='width: 100%'/> <br/>{% endif %}"
                                           vehiclesinformation="{{ emails_n.vehicle.vehiclesinformation.condition.name }}&nbsp;{{ emails_n.vehicle.vehiclesinformation.year.year }}&nbsp;{{ emails_n.vehicle.vehiclesinformation.make.makedisplay }}<br><span class='bold_text_black'>Price: ${{ emails_n.vehicle.vehiclesinformation.price|number_format() }}</span>"
                                           vehiclesinformation1="Milage: {{ emails_n.vehicle.vehiclesinformation.mileage }}<br/>Color: {{ emails_n.vehicle.vehiclesdetails.exteriorcolor }}<br/>Stock #: {{ emails_n.vehicle.vehiclesinformation.stockNumber }}<br/>"
                                           image_user="{% if emails_n.profile %}{% if(emails_n.profile.logo is not null) %}<div class='col-md-8' style='padding-left: 0px !important;'><div class='thumbnail'><div class='logo img-responsive' style='background-image:url('{{ asset(emails_n.profile.logo) }}');height: 120px;width:100%'></div></div></div>{% else %}<div class='col-md-8' style='padding-left: 0px !important;'><div class='thumbnail'><div class='logo img-responsive' style='background-image:url('{{ asset('bundles/common/images/image-bg.jpg') }}');height: 120px;width:100%'></div></div></div>{% endif %}<div class='caption col-md-12 col-sm-12 col-xs-12' style='padding-left: 0px'><a href='{{ path('profile_view',{'prop_pk':emails_n.profile.serie }) }}'class='blue_text'>{{ emails_n.profile.description.name }}</a></div>{% else %}<img class='col-md-6 col-sm-3 col-xs-5 logo img-responsive'src='{{ asset('bundles/common/images/for-sale.jpg') }}' style='padding-left: 0px'><div class='caption col-md-12 col-sm-12 col-xs-12' style='padding-left: 0px;padding-right: 0px'><a href='{{ path('my_account') }}'class='blue_text'>{{ emails_n.user.name }}</a></div>{% endif %}"
                                           email_inf1="<span class='date'> {{ emails_n.datecreated | date('M d, Y - h:i a') }}</span><br/><span class='bold_text_black'> {{ emails_n.remitentname }}:</span>"
                                           email_inf2="<span class=''>Email:</span> <span class='blue_link'>{{ emails_n.remitentMail }}</span><br/><span class=''>Phone:</span> <span class='blue_link'> {{ emails_n.remitentphone }}</span>"
                                           email_inf3=" {% if emails_n.offer %}<h3>Offer: ${{ emails_n.offertext }} </h3>{% else %}<span>{{ emails_n.bodymail }}</span>{% endif %}"
                                           data-toggle="modal"
                                           data-target="#myModalInquiryListRead"
                                           class="btn btn-default btn-block text-center text-uppercase read">
                                            <small>Read</small>
                                        </a>
                                        <a id="{{ emails_n.id }}" name="{{ emails_n.remitentname }}"
                                           offer="<h3>Offer: ${{ emails_n.offertext }} </h3>"
                                           body="{{ emails_n.bodymail }}"
                                           data-toggle="modal"
                                           data-target="#myModalInquiryListReply"
                                           class="btn btn-default btn-block text-center text-uppercase reply">
                                            <small>Reply</small>
                                        </a>
                                    </div>
                                    <div class="thumbnail" style="margin-bottom: 0px;">


                                        {% if emails_n.vehicle.gallery is not null %}
                                            {% set pf = emails_n.vehicle.gallery.vehicleimages[0] %}
                                            <a href="javascript:void(0)" class="thumbnail" style="margin-bottom: 0px ">
                                                <img src="{{ asset(pf.getwebpaththumbnailVehicle(user.id,emails_n.vehicle.serie)) }}"
                                                     alt=""
                                                     class="img-responsive"/>
                                            </a>
                                        {% else %}
                                            {% set pf = 0 %}
                                            <a href="javascript:void(0)" class="thumbnail" style="margin-bottom: 0px ">
                                                <img src="{{ asset('bundles/common/images/image-bg.jpg') }}"
                                                     alt=""
                                                     class="img-responsive" style="width: 100%;height: 201px"/>
                                            </a>
                                        {% endif %}


                                        <div class=" caption " style="padding-right: 0px;padding-left: 0px">
                                            <a href="#" class="pull-left blue_text"
                                               style="text-decoration: no-underline;">{{ emails_n.vehicle.vehiclesinformation.condition.name }} {{ emails_n.vehicle.vehiclesinformation.getYear() }} {{ emails_n.vehicle.vehiclesinformation.getMake() }} {{ emails_n.vehicle.vehiclesinformation.getModel().modelDisplay }}</a>
                                            <br>
                                            <span class="pull-left bold_text_black">${{ emails_n.vehicle.vehiclesinformation.price|number_format() }}</span>
                                        </div>

                                        <div class="row line_with_image" style="margin-top: 10px">
                                            <hr/>
                                            <a href="{{ path('user_email_content' ,{'mail_id':emails_n.id}) }}"
                                               mail_id="{{ emails_n.id }}"
                                               class="line_icon
                                            {% if emails_n.offer %}
                                            icon-offer
                                        {% else %}
                                            {% if emails_n.opened == 1 %}
                                                messageicon
                                            {% else %}
                                                messageicon_new
                                            {% endif %}
                                            {% endif %}"></a>
                                        </div>

                                        <div class="clearfix">
                                            <span class="date"> {{ emails_n.datecreated | date("M d, Y - h:i a") }}</span><br>
                                            <span class="user_name bold_text_black"
                                                  style="color: #000000;">{{ emails_n.remitentname }}{% if emails_n.offer %}'s Offer:{% endif %}</span><br>
                                            {% if emails_n.offer %}
                                                <span class="blue_text"
                                                      style="font-size: 14px ">${{ emails_n.offertext }}</span>
                                            {% else %}
                                                {% if  emails_n.bodymail|length < 35 %}
                                                    <span style="font-size: 14px ">{{ emails_n.bodymail }}...</span>
                                                {% else %}
                                                    {% set bodyMail = emails_n.bodymail|split('', 35) %}
                                                    <span style="font-size: 14px ">{{ bodyMail[0] }}...</span>
                                                {% endif %}
                                            {% endif %}
                                            </p>
                                        </div>
                                    </div>
                                </div>

                            </div>

                        {% endfor %}
                        {% set count = count + 3 %}
                    </div>
                {% endfor %}
            {% endif %}
        </div>
    </div>

</div>

