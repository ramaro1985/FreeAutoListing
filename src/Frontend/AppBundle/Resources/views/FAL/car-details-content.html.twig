<style>

    hr {
        width: 100%;
        margin-bottom: 0px;
        margin-top: 8px;
        border-width: thin;
        border-color: #000000;
    }

    .hr_text {
        position: relative;
        left: 47%;
        padding-left: 2px;
        padding-right: 2px;
        background-color: white;
        bottom: 11px;
    }

    .details dl {
        margin-bottom: 0px;
        margin-top: 5px;
    }

    .details span {
        margin-bottom: 6px;
    }

    .details dl > dt, dd {
        font-weight: normal;
        text-align: left;
    }

    .details dl dt {
        width: 108px;
        color: #555555;
    }

    .details dl dd {
        margin-left: 100px;
    }

    .nav-tabs li:hover {
        background-color: #ffffff;
    }

    .nav-tabs li a:hover {
        background-color: #ffffff;
    }

    #myTabContent {
        background-color: #ffffff;
        border: 1px solid #CCCCCC;
        border-top-color: transparent;
    }

    ul.nav-tabs {
        margin-bottom: 0px;
    }

    ul.nav-tabs li a {
        background-color: #ffffff;
        border: 1px solid #CCCCCC;
        border-radius: 0px;
        padding: 4px 15px
    }

    ul.nav-tabs li.active a {
        border: 1px solid #CCCCCC;
        border-bottom-color: transparent;
    }

    .list_styled {
        -webkit-padding-start: 32px;
        list-style-type: disc;
    }

    .list_styled li {
        display: list-item;
    }

    .media-buttons {
        margin-top: 0px;
        text-decoration: none;
        color: #fff;
        display: inline-block;
        border: none;
        height: 22px;
        font-weight: bold;
        font-size: 11px;
        color: white;
        text-decoration: none;
        font-family: "lucida grande", tahoma, verdana, arial, sans-serif;
        line-height: 22px;
        margin-top: 11px;
        cursor: pointer;
        margin-left: 4px;
    }

    .facebook_button {
        background: #627aac url({{ asset('bundles/common/images/fb.png') }}) left no-repeat;
        padding: 0px;
        width: 49px;
    }

    .twitter_button {
        background: #d0d0d0 url({{ asset('bundles/common/images/tw.png') }}) left no-repeat;
        padding: 0px;
        width: 58px;
        margin-left: 0px;
    }

    .googleplus_button {
        background: #ececec url({{ asset('bundles/common/images/g+.png') }}) left no-repeat;
        padding: 0px;
        width: 35px;
    }

</style>

<div class="container">
<div class="col-md-12 text-left">
    <a href="{{ path('search_car_results') }}"><< Back to Results</a>
</div>

<div class="col-xs-12 col-md-12 col-sm-12 no_padding">
<div class="col-md-9">
<div class="container-fluid">

    <div class="row">
        <div class="col-md-12 no_padding">

            <div class="col-md-6 text-right pull-right no_padding">
                <button type="button" class="btn black_buttons btn-xs">Compare</button>
                <button type="button" class="btn black_buttons btn-xs">Save Car</button>
                <a class="media-buttons facebook_button icon icon_twitter tip-top tooltipstered"
                   target="_blank"
                   rel="nofollow"
                   onclick="window.open(this.href,this.title,'width=500,height=500,top=300px,left=300px'); return false;"
                   href="http://www.facebook.com/plugins/like.php?href={{ path('car_details', {'serie': vehicle.serie}) }}">
                    </i>
                </a>
                <a class="media-buttons twitter_button icon icon_twitter tip-top tooltipstered" target="_blank"
                   rel="nofollow"
                   onclick="window.open(this.href,this.title,'width=500,height=500,top=300px,left=300px'); return false;"
                   href="https://twitter.com/share?url=https://www.freeautolisting.com{{ path('car_details', {'serie': vehicle.serie}) }}">
                    </i>
                </a>
                <a class="media-buttons googleplus_button icon icon_googleplus tip-top tooltipstered  pull-right pull-right"
                   target="_blank" rel="nofollow"
                   onclick="window.open(this.href,this.title,'width=500,height=500,top=300px,left=300px'); return false;"
                   href="//plus.google.com/share?url=https://www.freeautolisting.com{{ path('car_details', {'serie': vehicle.serie}) }}">
                    </i>
                </a>
            </div>

        </div>

    </div>
</div>
<div class="container-fluid">
<div class="row">
<div class="col-md-12 col-xs-12 col-sm-12 container_box no_padding">
<div class="col-md-8 col-xs-12 col-sm-10" style="padding-bottom: 15px;">
    <div class="col-md-12 no_padding">
        <h4>{{ vehicle.vehiclesinformation.condition.name }}
            {{ vehicle.vehiclesinformation.year.year }}
            {{ vehicle.vehiclesinformation.make.makeDisplay }}
            {{ vehicle.vehiclesinformation.model.modelDisplay }}</h4>
    </div>
    <div class="col-md-12 no_padding">
        <span>Mileage: {{ vehicle.vehiclesinformation.mileage }}</span>
    </div>

    <div class="col-md-12 no_padding" style="margin-top: 18px">
        {% if vehicle.profile %}
            {% if(vehicle.gallery is not null ) %}
                {% set pf = vehicle.gallery.profileimages[0] %}
                <div class="tumb-large"
                     style="height: 423px;background-image: url({{ asset(pf.getWebPathMediumVehicle(vehicle.profile,vehicle.serie)) }})">

                </div>
            {% else %}
                {% set pf = 0 %}
                <div class="tumb-large"
                     style="height: 423px;background-image: url({{ asset('bundles/common/images/image-bg.jpg') }})">

                </div>
            {% endif %}
        {% endif %}
        {% if vehicle.user %}
            {% if(vehicle.gallery is not null ) %}
                {% set pf = vehicle.gallery.vehicleimages[0] %}
                <div class="tumb-large"
                     style="height: 423px;background-image: url({{ asset(pf.getWebPathMediumVehicle(vehicle.user.id,vehicle.serie)) }})">

                </div>
            {% else %}
                {% set pf = 0 %}
                <div class="tumb-large"
                     style="height: 423px;background-image: url({{ asset('bundles/common/images/image-bg.jpg') }})">

                </div>
            {% endif %}
        {% endif %}
    </div>

    <div class="col-md-12 no_padding imagegallery" style="margin-top: 15px">

        {% if vehicle.profile %}
            {% if(vehicle.gallery is not null) %}
                {% for pf in vehicle.gallery.profileimages %}

                    <div class="image-form">
                        {% if pf.getWebPath is not null %}
                            <div class="tumb old active"
                                 style="padding-left: 1px;padding-right: 1px;padding-top: 2px;
                                         background-image:url('{{ asset(pf.getWebPathThumbnailVehicle(vehicle.profile,vehicle.serie)) }}');"></div>
                        {% else %}
                            <div id="gallery_propertyImages_{{ i }}" class="tumb no_image"
                                 style="background-image:url('{{ asset('bundles/common/images/image-bg.jpg') }}');"></div>
                        {% endif %}
                    </div>

                {% endfor %}

                {% if  vehicle.gallery.profileimages|length < 9 %}
                    {% for i in vehicle.gallery.profileimages|length..9 %}

                        <div class="image-form">
                            <div id="gallery_propertyImages_{{ i }}" class="tumb no_image"
                                 style="background-image:url('{{ asset('bundles/common/images/image-bg.jpg') }}');"></div>
                        </div>

                    {% endfor %}


                {% endif %}
            {% else %}

                {% for i in 0..9 %}

                    <div class="image-form">
                        <div id="gallery_propertyImages_{{ i }}" class="tumb no_image"
                             style="background-image:url('{{ asset('bundles/common/images/image-bg.jpg') }}');"></div>
                    </div>

                {% endfor %}
            {% endif %}

        {% endif %}

        {% if vehicle.user %}

            {% if(vehicle.gallery is not null) %}
                {% for pf in vehicle.gallery.vehicleimages %}

                    <div class="image-form">
                        {% if pf.getWebPath is not null %}
                            <div class="tumb old active"
                                 style="padding-left: 1px;padding-right: 1px;padding-top: 2px;
                                         background-image:url('{{ asset(pf.getWebPathThumbnailVehicle(vehicle.user.id,vehicle.serie)) }}');"></div>
                        {% else %}
                            <div id="gallery_propertyImages_{{ i }}" class="tumb no_image"
                                 style="background-image:url('{{ asset('bundles/common/images/image-bg.jpg') }}');"></div>
                        {% endif %}
                    </div>

                {% endfor %}

                {% if  vehicle.gallery.vehicleimages|length < 9 %}
                    {% for i in vehicle.gallery.vehicleimages|length..9 %}

                        <div class="image-form">
                            <div id="gallery_propertyImages_{{ i }}" class="tumb no_image"
                                 style="background-image:url('{{ asset('bundles/common/images/image-bg.jpg') }}');"></div>
                        </div>

                    {% endfor %}
                {% endif %}
            {% else %}

                {% for i in 0..9 %}

                    <div class="image-form">
                        <div id="gallery_propertyImages_{{ i }}" class="tumb no_image"
                             style="background-image:url('{{ asset('bundles/common/images/image-bg.jpg') }}');"></div>
                    </div>

                {% endfor %}
            {% endif %}
        {% endif %}


    </div>

</div>

<div class="col-md-4 no_padding" style="padding-left: 0px">

    <div class="container-fluid">
        <div class="col-md-12 no_padding pull-left"
             style="border-bottom: 1px solid black"><span
                    style="padding-top: 15px;padding-bottom: 15px;">
                                        Price:<span class="bold_text_black"
                                                    style="font-size: 25px;">${{ vehicle.vehiclesinformation.price|number_format() }}</span>
                                    </span></div>

        <div class="col-md-12 no_padding pull-left">
            <div class="thumbnail col-md-5 no_padding">
                {% if(vehicle.profile) %}
                    <a href="{{ path('dealer-info', {'profile': vehicle.profile.serie}) }}">
                        <div class="logo img-responsive"
                             style="margin-top: 2px;margin-bottom: 2px;background-image:url('{{ asset( vehicle.profile.logo) }}');height: 85px;width:100%">
                        </div>
                    </a>
                {% elseif vehicle.user %}
                    <div class="logo img-responsive"
                         style="margin-top: 2px;margin-bottom: 2px;background-image:url('{{ asset( 'bundles/common/images/for-sale.jpg' ) }}');height: 85px;width:100%">
                    </div>
                {% endif %}
            </div>
            <div class="col-md-7" style="padding-top: 5px">
                {% if(vehicle.profile) %}
                    <div class="col-md-12 no_padding caption"
                         style="padding-left: 0px;padding-right: 0px;">
                        <a style="text-decoration: none"
                           href="{{ path('dealer-info', {'profile': vehicle.profile.serie}) }}"><h5
                                    class="textlogo">{{ vehicle.profile.description.name }}</h5></a>
                    </div>
                {% elseif vehicle.user %}
                    <div class="col-md-12 no_padding caption"
                         style="padding-left: 0px;padding-right: 0px;">
                        <h5 style="font-weight: bold;margin-top: 0px">Private Seller</h5>
                        <span style="width: 100%">{{ vehicle.user.name }}</span>
                    </div>
                {% endif %}
            </div>
        </div>

        <div class="col-md-12 no_padding clearfix" style="padding-top: 8PX">
            {% if vehicle.profile %}
                <span style="width: 100%">{{ vehicle.profile.description.phone }}</span>
                <br/>
                <span style="width: 100%">{{ vehicle.profile.description.address }} {{ vehicle.vehiclesinformation.zipcode }}</span>
            {% endif %}
            {% if vehicle.user %}
                <span style="width: 100%">{{ vehicle.user.phone }}</span>
            {% endif %}
        </div>

        <div class="col-md-12 no_padding" style="margin-top: 15px">
            <div class="panel panel-default" style="border: none;font-size: 12px">
                <form id="form-description"
                      action="{{ path('car_details',{'serie': vehicle.serie}) }}" {{ form_enctype(form) }}
                      method="POST" class="form-horizontal">
                    <fieldset>
                        <div class="panel-heading" style="padding-left: 0px;">
                            <h2 class="panel-title"><strong>Email this seller</strong></h2>
                        </div>
                        <div class="panel-body"
                             style="padding-bottom: 0px;padding-top: 0px;">
                            <div class="form-group">
                                {{ form_widget(form.remitent_name) }}
                            </div>
                            <div class="form-group">
                                {{ form_widget(form.remitent_mail) }}
                            </div>
                            <div class="form-group">
                                {{ form_widget(form.remitent_phone) }}
                            </div>
                            <div class="form-group">
                                {{ form_widget(form.body_mail) }}
                            </div>
                            <div class="form-group">
                                <div class="checkbox"
                                     style="padding-top: 0px;font-size: 12px">
                                    <label>
                                        <input type="checkbox"> I would like to receives
                                        updates, and promotions.
                                    </label>
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="row hide" id="loading" style="margin-left: 2px">
                                    <img id="loading"
                                         src="{{ asset('bundles/common/images/loading.gif') }}"
                                         style="font-size:12px;  border: 0 none;" class="">
                                    &nbsp; Sending
                                    Message...
                                </div>
                            </div>
                            <!-- <div class="form-group">
                                 <div class="alert hide" id="succes_mess"
                                      style="background-color: #0cffdd">
                                     <button type="button" class="close"
                                             data-dismiss="alert">&times;</button>

                                                             <strong>Succes!</strong> Your message has been submited
                                                             succesfully....
                                                         </div>
                                                     </div>-->
                            <a href="javascript:void(0)" id="submit_mail"
                               class="btn btn-default text-uppercase btn-toolbar active col-md-12"
                               style="margin-left: 0px">
                                SEND MESSAGE </a>


                            <div class="col-md-12 no_padding">

                                <hr style="width: 100%;margin-bottom: 0px;"/>
                                <span class="hr_text">or</span>

                            </div>

                            <a href="javascript:void(0)" id="create_offer"
                               data-toggle="modal"
                               data-target="#myModalCar"
                               class="btn btn-default text-uppercase btn-danger col-md-12">
                                MAKE AN OFFER
                            </a>
                        </div>
                    </fieldset>
                    {{ form_rest(form) }}
                </form>
            </div>
        </div>

    </div>

</div>
</div>
<div class="modal fade" id="myModalCar" tabindex="-1" role="dialog"
     aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog" style="max-width: 400px;">
        <div class="modal-content">
            <div class="modal-header" style="background-color: lightgrey">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">Make an Offer</h4>
            </div>
            <div class="modal-body">
                <div class="container-fluid">
                    <form id="offerVehicle"
                          action="{{ path('car_details',{'serie': vehicle.serie}) }}"
                          method="POST"
                          class="form-horizontal">

                        <fieldset>
                            <div class="col-md-12" style="padding: 0px 33px;">

                                <div class="">
                                    <br>
                                    <br>

                                    <div class="form-group no_padding">
                                        <ul class="no_padding list-unstyled">
                                            <li>
                                                <span style="font-size: 16px;font-weight: bold; font-style: inherit;">Listed Price:&nbsp;${{ vehicle.vehiclesinformation.price|number_format() }}</span>
                                            </li>
                                            <li>
                                                                    <span style="font-size: 14px;">
                                                                    {{ vehicle.vehiclesinformation.condition.name|upper }}
                                                                        &nbsp;{{ vehicle.vehiclesinformation.year.year|upper }}
                                                                        &nbsp;{{ vehicle.vehiclesinformation.make.makeDisplay|upper }}
                                                                        &nbsp; {{ vehicle.vehiclesinformation.model.modelDisplay|upper }}
                                                                </span>
                                            </li>
                                            <li>
                                                <span style="font-size: 14px;">Milage:&nbsp;{{ vehicle.vehiclesinformation.mileage }}</span>
                                            </li>

                                        </ul>
                                    </div>
                                    <div class="form-group">
                                        {{ form_widget(form_offer.remitent_name) }}
                                    </div>
                                    <div class="form-group">
                                        {{ form_widget(form_offer.remitent_mail) }}
                                    </div>
                                    <div class="form-group">
                                        {{ form_widget(form_offer.remitent_phone) }}
                                    </div>
                                    <div class="form-group bold_text_black">
                                        <div class="col-md-12 no_padding">
                                            <span style="position: absolute;top: 25px;left: -13px;">$</span>
                                            <span style="font-style: inherit;font-weight: bold">Your Offer</span>
                                            {{ form_widget(form_offer.text) }}
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="row hide" id="loadingOff"
                                             style="margin-left: 2px">
                                            <img id="loading"
                                                 src="{{ asset('bundles/common/images/loading.gif') }}"
                                                 style="font-size:12px;  border: 0 none;" class="">
                                            &nbsp; Sending
                                            Message...
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="alert hide" id="succes_offer"
                                             style="background-color: #0cffdd">
                                            <button type="button" class="close"
                                                    data-dismiss="alert">&times;</button>

                                            <strong>Succes!</strong> Your Offer has been submited
                                            succesfully....
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <br>
                                        <br>

                                        <div class="col-md-9 col-sm-9 col-xs-9"
                                             style="padding-left: 0px">
                                            <a href="javascript:void(0)" id="submit_offer"
                                               class="btn btn-default btn-block text-uppercase btn-danger col-md-7">
                                                MAKE A OFFER
                                            </a>
                                        </div>
                                        <div class="col-md-2 col-sm-2 col-xs-2"
                                             style="padding-left: 0px">
                                            <button type="button"
                                                    class="btn btn-toolbar close-btn text-uppercase"
                                                    data-dismiss="modal"
                                                    style="background-color: white;color: blue;">
                                                Cancel
                                            </button>
                                        </div>
                                    </div>
                                </div>

                            </div>
                            {{ form_rest(form_offer) }}
                            <br>
                        </fieldset>
                    </form>
                </div>
            </div>

        </div>
    </div>
</div>
<div class="col-md-12 col-xs-12 col-sm-12" style="margin-top: 20px;margin-bottom: 20px">
