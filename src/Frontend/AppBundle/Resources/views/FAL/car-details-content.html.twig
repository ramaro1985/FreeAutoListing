                                        </div>
                                    </div>
                                </div>
                            </a>

                        {% endif %}

                        {% if(car.user) %}
                            <a href=" {{ path('car_details', {'serie': car.serie}) }} ">
                                <div class="container-fluid panel_box"
                                     style="background-color: white">
                                    <div class="thumbnail">
                                        <div class="col-md-5 no_padding">
                                            {% if(car.gallery is not null) %}
                                                {% set pf = car.gallery.vehicleimages[0] %}

                                                <div class="similar_car_tumb old"
                                                     style="padding-left: 1px;padding-right: 1px;padding-top: 2px;
                                                             background-image:url('{{ asset(pf.getWebPathThumbnailVehicle(car.user.id,car.serie)) }}');"></div>
                                            {% else %}
                                                <div id="gallery_propertyImages"
                                                     class="similar_car_tumb no_image"
                                                     style="background-image:url('{{ asset('bundles/common/images/image-bg.jpg') }}');"></div>
                                            {% endif %}
                                        </div>
                                        <div class="col-md-7 caption">

                                            <a>{{ car.vehiclesinformation.condition.name }}
                                                {{ car.vehiclesinformation.year.year }}
                                                {{ car.vehiclesinformation.make.makeDisplay }}</a><br/>
                                            <a>{{ car.vehiclesinformation.model.modelDisplay }}</a>

                                            <p>
                                                <span class="price">${{ car.vehiclesinformation.price|number_format() }}</span><br/>
                                                <span>{{ car.user.name }}</span><br/>
                                                <span>{{ car.vehiclesdetails.enginetype }}</span>
                                            </p>

                                        </div>
                                    </div>
                                </div>
                            </a>
                        {% endif %}

                    {% endfor %}

                    <div class="col-md-12">
                        <div class="container-fluid text-right">
                            <a id="moreOptions" href="javascript:void(0)"> More Options <span
                                        class="glyphicon glyphicon-triangle-right"></span> </a>
                        </div>
                    </div>

                </div>

            </div>
        </div>
    </div>
</div>
</div>
</div>

<script type="text/javascript">
    jQuery(document).ready(function () {

                function createPopover(elem, text) {
                    elem.attr('data-toggle', 'popover');
                    elem.attr('data-placement', 'right');
                    // elem.attr('data-placement', 'bottom');
                    elem.attr('data-content', text);
                    elem.attr('data-container', 'body');
                    elem.popover();
                    elem.trigger('click');
                    elem.focus();
                }

                function disableForm(formulario) {
                    if (formulario == 'mail') {
                        jQuery('#form-description').find('input ,button, textarea').attr('disabled', 'disabled')
                        jQuery("#form-description").css("opacity", 0.37);
                        jQuery("#loading").removeClass("hide");
                        jQuery("#succes_mess").addClass("hide");

                    } else if (formulario == 'offer') {
                        jQuery('#offerVehicle').find('input ,button').attr('disabled', 'disabled')
                        jQuery("#offerVehicle").css("opacity", 0.37);
                        jQuery("#loadingOff").removeClass("hide");
                        jQuery("#succes_offer").addClass("hide");
                    }


                }

                function resetForm(formulario) {
                    if (formulario == 'mail') {
                        jQuery('#form-description').find('input, textarea').val('');
                    } else if (formulario == 'offer') {
                        jQuery('#offerVehicle').find('input, textarea').val('');
                    }


                }

                function enableForm(formulario) {
                    if (formulario == 'mail') {
                        jQuery('#form-description').find('input , button, textarea').removeAttr('disabled')
                        jQuery("#form-description").css("opacity", 1);
                    } else if (formulario == 'offer') {
                        jQuery('#offerVehicle').find('input , button').removeAttr('disabled')
                        jQuery("#offerVehicle").css("opacity", 1);
                    }

                }

                function checkSelected(o) {
                    if (o.val() == 0) {
                        createPopover(o, 'Please fill out this field');
                        return false;
                    } else {
                        return true;
                    }
                }

                function checkLength(o, min, max) {
                    if (o.val().length < min) {
                        createPopover(o, 'Please fill out this field');

                        return false;
                    } else {
                        return true;
                    }
                }


                function checkRegexp(o, regexp) {
                    if (!( regexp.test(o.val()) )) {
                        createPopover(o, 'Invalid field');
                        return false;
                    } else {
                        return true;
                    }
                }

                function validateForm(formulario) {
                    var bValid = true;

                    if (formulario == 'mail') {
                        bValid = bValid && checkSelected(jQuery('#email_remitent_name'));
                        bValid = bValid && checkLength(jQuery('#email_remitent_name'), 4, 100);


                        /*bValid = bValid && checkSelected(jQuery('#email_remitent_last_name'));
                         bValid = bValid && checkLength(jQuery('#email_remitent_last_name')), 4, 100;*/

                        bValid = bValid && checkSelected(jQuery('#email_remitent_mail'));
                        bValid = bValid && checkSelected(jQuery('#email_body_mail'));
                        bValid = bValid && checkLength(jQuery('#email_body_mail'), 2, 240);
                        bValid = bValid && checkLength(jQuery('#email_remitent_mail'), 4, 100);
                        bValid = bValid && checkRegexp(jQuery('#email_remitent_mail'), /^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i);

                    } else if (formulario == 'offer') {
                        bValid = bValid && checkSelected(jQuery('#offer_remitent_name'));
                        bValid = bValid && checkLength(jQuery('#offer_remitent_name'), 4, 100);

                        bValid = bValid && checkSelected(jQuery('#offer_remitent_mail'));
                        bValid = bValid && checkLength(jQuery('#offer_remitent_mail'), 4, 100);

                        bValid = bValid && checkSelected(jQuery('#offer_text'));
                        bValid = bValid && checkLength(jQuery('#offer_text'), 1, 100);

                        bValid = bValid && checkRegexp(jQuery('#offer_remitent_mail'), /^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i);

                    }

                    if (bValid) {
                        disableForm(formulario);
                        submitForm(formulario);
                    }

                }

                function submitForm(formulario) {
                    //var remitent_last_name;
                    var remitent_mail;
                    var remitent_phone;
                    var remitent_name;
                    var body_mail;
                    var offer_text;

                    // alert(offer_tetx = jQuery('#offer_tetx').val());

                    if (formulario == 'mail') {
                        remitent_name = jQuery('#email_remitent_name').val();
                        //remitent_last_name = jQuery('#email_remitent_last_name').val();
                        remitent_mail = jQuery('#email_remitent_mail').val();
                        remitent_phone = jQuery('#email_remitent_phone').val();
                        body_mail = jQuery('#email_body_mail').val();
                        offer_text = "";

                    } else if (formulario == 'offer') {
                        remitent_name = jQuery('#offer_remitent_name').val();
                        //remitent_last_name = '.';
                        remitent_mail = jQuery('#offer_remitent_mail').val();
                        remitent_phone = jQuery('#offer_remitent_phone').val();
                        body_mail = "";
                        offer_text = jQuery('#offer_text').val();
                    }
                    jQuery.ajax({
                        url: jQuery('#form-description').attr('action'),
                        type: "POST",
                        data: {
                            formulario: formulario,
                            remitent_name: remitent_name,
                            //remitent_last_name: remitent_last_name,
                            remitent_mail: remitent_mail,
                            remitent_phone: remitent_phone,
                            body_mail: body_mail,
                            offer_text: offer_text
                        },
                        async: false,
                        success: function (response) {
                            resetForm(formulario);
                            enableForm(formulario);
                            if (formulario == 'mail') {
                                jQuery("#loading").addClass("hide");
                                jQuery("#succes_mess").removeClass("hide");
                            } else {
                                jQuery("#loadingOff").addClass("hide");
                                jQuery("#succes_offer").removeClass("hide");
                            }

                            jQuery("#myModalCar").modal('destroy');
                        }
                    });

                }

                jQuery('#submit_mail').click(function () {
                    validateForm('mail');
                });
                jQuery('#submit_offer').click(function () {
                    validateForm('offer');
                });

                jQuery(".tumb").click(function () {
                    if (!jQuery(this).hasClass("no_image")) {
                        var image = jQuery(this).css("background-image");
                        jQuery(".tumb-large").css("background-image", image);
                    }
                })
            }
    )
</script>