            </div>

        </div>
        <div class="col-md-12 col-sm-6 col-md-12 no_padding" style="background-color: #F1F1F1">
            <div class="panel panel-default">
                <div class="panel-heading" style="border-bottom: 1px solid #AAA5A5">
                    <h2 class="panel-title text-uppercase"><strong>SIMILAR MODELS</strong></h2>
                </div>
                <div class="panel-body"
                     style="background-color: rgb(241, 241, 241);padding: 20px 10px 10px 10px">

                    {% for car in similars|slice( 0, 3 ) %}

                        {% if(car.profile) %}

                            <a href=" {{ path('car_details', {'serie': car.serie}) }} ">
                                <div class="container-fluid panel_box"
                                     style="background-color: white">
                                    <div class="thumbnail">
