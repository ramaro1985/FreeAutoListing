        <li id="overview_tab" class="active">
            <a aria-expanded="false" href="#overview" data-toggle="tab">
                <span class="glyphicon overview_icon"></span>
                OVERVIEW</a>
        </li>
        {#<li id="modelsdetails_tab" class="text-uppercase">
            <a aria-expanded="true" href="#modelsdetails" data-toggle="tab">
                <span class="glyphicon details_icon"></span>
                Models Details</a></li>
        <li id="reviews_tab" class="text-uppercase">
