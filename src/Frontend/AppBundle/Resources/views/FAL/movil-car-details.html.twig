{% extends "CommonBundle:Default:base.html.twig" %}
  {% block title %} Free Auto Listing  {% endblock %}
{% block stylesheets %}
    {{ parent() }}
    <link href="{{ asset('bundles/common/css/car-details-style.css') }}" rel="stylesheet"/>
    <link href="{{ asset('bundles/common/css/movil-search-result-style.css') }}" rel="stylesheet"/>
    <style type="text/css">
        #header {
            background: none repeat scroll 0 0 #FFFFFF;
            left: 0;
            position: relative;
            right: 0;
            top: 0;
            width: 100%;
            z-index: 2100;
            border-bottom: 1px solid black;
            margin-bottom: 0px;
        }
        hr {
            width: 100%;
            margin-bottom: 0px;
            border-width: thin;
            border-color: #ddd;
        }

        .hr_text {
            position: relative;
            left: 47%;
            padding-left: 2px;
            padding-right: 2px;
            background-color: white;
            bottom: 9px;
        }

        .image-form {
            background-color: rgb(52, 51, 51);
            width: 16%;
            padding-top: 3px;
            padding-bottom: 3px;;
        }

        .tumb {
            height: 57px;
        }
    </style>
{% endblock %}
