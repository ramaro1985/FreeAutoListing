<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {% block metadescriptions %}{% endblock %}
    <title>{% block title %}My Dashboard{% endblock %}</title>

    {% block stylesheets %}
        <link href="{{ asset('bundles/common/css/bootstrap.css') }}" rel="stylesheet"/>
        <link href="{{ asset('bundles/common/css/jquery-ui-1.10.4.custom.min.css') }}" rel="stylesheet"/>
        <link href='https://fonts.googleapis.com/css?family=Quattrocento+Sans' rel='stylesheet' type='text/css'>
        <link href="{{ asset('bundles/common/css/dashboard_style.css') }}" rel="stylesheet"/>
        <link href="{{ asset('bundles/common/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/common/css/jRating.jquery.min.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/common/css/fullcalendar.min.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/common/css/fullcalendar.print.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/common/css/fal-style.css') }}" rel="stylesheet"/>
    {% endblock %}
    {% block javascripts %}
        <script src="{{ asset('bundles/common/js/jquery-1.11.3.min.js') }}"></script>
        <script src="{{ asset('bundles/common/js/bootstrap.js') }}"></script>
        <script src="{{ asset('bundles/common/js/jquery-ui-1.10.4.min.js') }}"></script>
        <script src="{{ asset('bundles/common/js/transition.js') }}"></script>
        <script src="{{ asset('bundles/common/js/jRating.jquery.min.js') }}"></script>
        <script src="{{ asset('bundles/common/js/fullcalendar.js') }}"></script>
        <script src="{{ asset('bundles/common/js/tooltip.js') }}"></script>
        <script src="{{ asset('bundles/common/js/popover.js') }}"></script>
        <script src="{{ asset('bundles/common/js/jquery.bxslider.min.js') }}"></script>
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('bundles/common/images/free-auto.png') }}"/>
</head>
<style>
    #header {
        background-color: #fff;
        box-shadow: 0 2px 6px -2px rgba(0, 0, 0, 0.2);
        clear: both;
        margin: 0;
        padding-left: 0;
        padding-right: 0;
        position: static;
        width: 100%;
        z-index: 2;
    }
    .navbar-nav {
        float: left;
        margin: 0;
    }
    a {
        color: #337ab7;
        text-decoration: none;
    }
    .container {
        margin-left: auto;
        margin-right: auto;
        padding-left: 15px;
        padding-right: 15px;
    }
</style>
<body>
{{ include('CommonBundle:Default:header-cars.html.twig') }}
<div class="container" style="margin-top: 20px;">
    <div class="row">

        <div class="col-md-4 col-xs-12 menu-owner no_padding" style="padding-top: 30px">
            {% block menu %}{% endblock %}
        </div>
        <div class="col-md-8 col-xs-12 content-owner" style="min-height: 700px;padding-top: 5px">
            <div id="content-page">{% block body %}{% endblock %}</div>
        </div>

    </div>

</div>
{% block footer %}
{% endblock %}


</body>
</html>
