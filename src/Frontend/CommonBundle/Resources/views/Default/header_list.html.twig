            jQuery('.movile-apply').click(function (e) {
                if (validateFormMovil()) {

                    var listProperty = [];
                    var listLocation = [];
                    var listAmenities = [];
                    jQuery('.propertyM input:checked').each(function () {
                        listProperty.push(this.name);
                    });

                    jQuery('.locationM input:checked').each(function () {
                        listLocation.push(this.name);
                    });

                    jQuery('.amenitiesM input:checked').each(function () {
                        listAmenities.push(this.name);
                    });

                    jQuery('#moreProperty').val(listProperty);
                    jQuery('#moreLocation').val(listLocation);
                    jQuery('#moreAmenities').val(listAmenities);

                    e.preventDefault();
                    jQuery('.submit-movil-form').trigger('click');
                }

            });




            jQuery(document).scroll(function () {
               jQuery('#price').webuiPopover('hide');	
               jQuery('#sleeps').webuiPopover('hide');	
               jQuery('#bedrooms').webuiPopover('hide');	
               jQuery('#bathrooms').webuiPopover('hide');	
               jQuery('#more').webuiPopover('hide');	
            });

            




        </script>